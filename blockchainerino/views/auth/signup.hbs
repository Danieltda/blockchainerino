<h1 class="sign-up" id="signup-header">Sign Up</h1>
<form class="sign-up" id="signup-form" action="/auth/signup" method="POST">
  <label>
    Username
    <br>
    <input id="label-username" type="text" name="username" value = "{{username}}"/>
  </label>
  <br />
    <label>
    Email
    <br>
    <input id="label-email" type="email" name="email" value="{{email}}" />
  </label>
  <br />
  <label>
        <br>
    Password
    <br>
    <input id="label-password" type="password" name="password" />
  </label>
  <br />
  <button type="submit">Submit</button>
</form>
<div class="errorMessage">
{{#if errorMessage }}
  <p style="color:rgb(206, 240, 16)">{{errorMessage}}</p>
{{/if}}
</div>